import java.util.*;
public class Search {
	public static void main(String[] args)
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		int search=sc.nextInt();
		int temp=0;
		for(int i=0;i<n;i++)
		{
			arr[i]=sc.nextInt();
		}
		for(int i=0;i<n;i++)
		{
			if(arr[i]==search)
			{
				temp=-1;
				System.out.println("element found at "+i);
				break;
			}
		}
		if(temp!=-1)
		{
			System.out.println("element not found"+search);
		}
	}
}
